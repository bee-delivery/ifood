{
    "name": "bee-delivery/ifood",
    "description": "Integração rápida e fácil com o ifood, em PHP/Laravel",
    "authors": [
        {
            "name": "Sara Souza",
            "email": "sara.souza.9@hotmail.com"
        }
    ],
    "repositories":[
